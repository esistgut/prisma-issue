datasource db {
  url      = "file:data.db"
  provider = "sqlite"
}

generator client {
  provider = "prisma-client-js"
}


model Service {
  id          Int     @default(autoincrement()) @id
  owner       User    @relation
  code        String
  description String?

  @@unique([owner, code])
}


model User {
  id    Int    @default(autoincrement()) @id
  name  String @default("")
}